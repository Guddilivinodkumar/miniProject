
public class array {
      public void printarray(int arr[]) {
    	  int n=arr.length;
    	  for (int i=0;i<n;i++) {
    		  System.out.println(arr[i]+ " ");
    	  }
      }
      public  void arrayDemo() {
    	  int A[]=new int[5];
    	  printarray(A);
      }
      
      
      public static void main(String[] args) {
		array arr=new array();
		arr.arrayDemo();
	}
}
