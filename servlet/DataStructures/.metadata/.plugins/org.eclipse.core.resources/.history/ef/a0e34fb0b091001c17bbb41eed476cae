
public class array {
      public void printarray(int arr[]) {
    	  int n=arr.length;
    	  for (int i=0;i<n;i++) {
    		  System.out.printf(arr[i]+" ");
    	  }
    	  System.out.println();
      }   
      public  void arrayDemo() {
//    	  int A[]=new int[5];
//    	  int j=0;
//    	  for(int i=0;i<A.length;i++) {
//    		  A[i]=j++;
//    	  }
    	  printarray(new int[] {1,4,2,3});
      }
      
      
      public static void main(String[] args) {
		array arr=new array();
		arr.arrayDemo();
	}
}
